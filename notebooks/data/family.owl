<?xml version="1.0"?>
<rdf:RDF xmlns="http://mowl/family#"
     xml:base="http://mowl/family"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:http="http://"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://mowl/family"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://hasChild -->

    <owl:ObjectProperty rdf:about="http://hasChild"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://Father -->

    <owl:Class rdf:about="http://Father">
        <rdfs:subClassOf rdf:resource="http://Male"/>
    </owl:Class>
    


    <!-- http://Female -->

    <owl:Class rdf:about="http://Female">
        <owl:equivalentClass>
            <owl:Class>
                <owl:complementOf rdf:resource="http://Male"/>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://Person"/>
        <owl:disjointWith rdf:resource="http://Male"/>
    </owl:Class>
    


    <!-- http://Male -->

    <owl:Class rdf:about="http://Male">
        <rdfs:subClassOf rdf:resource="http://Person"/>
    </owl:Class>
    


    <!-- http://Mother -->

    <owl:Class rdf:about="http://Mother">
        <rdfs:subClassOf rdf:resource="http://Female"/>
    </owl:Class>
    


    <!-- http://Parent -->

    <owl:Class rdf:about="http://Parent">
        <rdfs:subClassOf rdf:resource="http://Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hasChild"/>
                <owl:someValuesFrom rdf:resource="http://Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://Person -->

    <owl:Class rdf:about="http://Person">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://Female"/>
                    <rdf:Description rdf:about="http://Male"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://Jane -->

    <owl:NamedIndividual rdf:about="http://Jane">
        <rdf:type rdf:resource="http://Mother"/>
        <http:hasChild rdf:resource="http://Melissa"/>
        <http:hasChild rdf:resource="http://Robert"/>
    </owl:NamedIndividual>
    


    <!-- http://John -->

    <owl:NamedIndividual rdf:about="http://John">
        <rdf:type rdf:resource="http://Father"/>
        <http:hasChild rdf:resource="http://Melissa"/>
        <http:hasChild rdf:resource="http://Robert"/>
    </owl:NamedIndividual>
    


    <!-- http://Melissa -->

    <owl:NamedIndividual rdf:about="http://Melissa">
        <rdf:type rdf:resource="http://Female"/>
    </owl:NamedIndividual>
    


    <!-- http://Robert -->

    <owl:NamedIndividual rdf:about="http://Robert">
        <rdf:type rdf:resource="http://Male"/>
    </owl:NamedIndividual>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="http://Female"/>
            <rdf:Description rdf:about="http://Parent"/>
        </owl:intersectionOf>
        <rdfs:subClassOf rdf:resource="http://Mother"/>
    </owl:Class>
    <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="http://Male"/>
            <rdf:Description rdf:about="http://Parent"/>
        </owl:intersectionOf>
        <rdfs:subClassOf rdf:resource="http://Father"/>
    </owl:Class>
</rdf:RDF>



